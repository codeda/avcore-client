  "scripts": {
    "tsc": "cd front;tsc",
    "browserify": "browserify ./front/dist/front/src/index.js -o ./front/dist/index.js",
    "build": "cd front && webpack --env.development",
    "test": "ts-node ./mediasoupServer.ts  >~/mediasoupServer.log 2>&1 &",
    "service": "ts-node ./mediasoupServer.ts  >~/mediasoupServer.log 2>&1",
    "start": "ts-node ./mediasoupServer.ts",
    "kill": "ps -ef | grep mediasoupServer.ts | grep -v grep | awk '{print $2}' | xargs kill -9",
    "copylog": "if [ -f ~/mediasoupServer.log ]; then cp ~/mediasoupServer.log ~/mediasoupServer-$(date +%Y-%m-%d-%H-%M).log; fi",
    "restart": "npm run kill;npm run copylog;npm test",
    "killffmpegs": "ps -ef | grep ffmpeg | grep -v grep | awk '{print $2}' | xargs kill -9"
  }
